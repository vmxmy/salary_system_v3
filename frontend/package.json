{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "NODE_OPTIONS='--max-old-space-size=1024' npx tsc -b && NODE_OPTIONS='--max-old-space-size=1024' vite build",
    "build:prod": "NODE_ENV=production npm run build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:cdn": "VITE_USE_CDN=true npm run build:prod",
    "build:optimized": "NODE_OPTIONS='--max-old-space-size=1024 --max-semi-space-size=128' npx tsc -b tsconfig.low-memory.json && NODE_OPTIONS='--max-old-space-size=1024 --max-semi-space-size=128' vite build",
    "build:minimal": "NODE_OPTIONS='--max-old-space-size=768 --max-semi-space-size=64' VITE_MINIMAL_BUILD=true npx tsc -b tsconfig.low-memory.json && NODE_OPTIONS='--max-old-space-size=768 --max-semi-space-size=64' VITE_MINIMAL_BUILD=true vite build",
    "build:legacy": "NODE_OPTIONS='--max-old-space-size=2048' npx tsc -b && NODE_OPTIONS='--max-old-space-size=2048' vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:prod": "NODE_ENV=production vite preview",
    "i18n:check": "node scripts/check-i18n.js",
    "i18n:check-ui": "node scripts/check-ui-i18n.js",
    "i18n:scan": "npm run i18n:check-ui"
  },
  "dependencies": {
    "@assistant-ui/react": "^0.10.43",
    "@assistant-ui/react-markdown": "^0.10.9",
    "@fontsource/crimson-text": "^5.2.6",
    "@fontsource/inter": "^5.2.6",
    "@fontsource/jetbrains-mono": "^5.2.6",
    "@fontsource/noto-sans-sc": "^5.2.6",
    "@fontsource/noto-serif-sc": "^5.2.6",
    "@fontsource/source-serif-pro": "^5.2.5",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.50.3",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/react-window": "^1.8.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "exceljs": "^4.4.0",
    "framer-motion": "^12.23.9",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.6.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.3",
    "react-window": "^1.8.11",
    "recharts": "^3.1.2",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "xlsx": "^0.18.5",
    "zod": "^4.0.2",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/exceljs": "^0.5.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "daisyui": "^5.0.46",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "glob": "^11.0.3",
    "globals": "^16.2.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.1.11",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "vite-plugin-compression": "^0.5.1"
  }
}
